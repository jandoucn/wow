<Bindings>
	<Binding name="A281_SHOW" description="" header="A281">
		A281_Show();
	</Binding>
	
	<Binding name="A281_STARTSTOP" >
		A281_Start();
	</Binding>
	

	<Binding name="A281_RUN" >
		A281_Run();
	</Binding>
	
	<Binding name="A281_AUTO" >
		A281_Auto();
	</Binding>
	
	<Binding name="A281_BURSTS"  runOnUp="true">
		
		
		if ( keystate == "down" ) then
			A281_Auto();
		else
			A281_Stop();
		end
		
	</Binding>
	
	<Binding name="A281_STOPALL" >
		A281_Stop();
	</Binding>
	
	
	
